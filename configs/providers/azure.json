{
  "provider": "azure",
  "name": "Azure OpenAI",
  "display_name": "Azure OpenAI",
  "description": "Microsoft Azure hosted OpenAI models with enterprise features",
  "base_url": "https://your-resource.openai.azure.com",
  "auth_type": "api-key",
  "request_format": "openai",
  "response_format": "openai",
  "cost_per_request": 0.002,
  "endpoints": [
    "/openai/deployments/{deployment-name}/chat/completions",
    "/openai/deployments/{deployment-name}/completions",
    "/openai/deployments/{deployment-name}/embeddings"
  ],
  "models": [
    {
      "name": "gpt-4",
      "display_name": "GPT-4 (Azure)",
      "description": "GPT-4 model hosted on Azure"
    },
    {
      "name": "gpt-4-32k",
      "display_name": "GPT-4 32K (Azure)",
      "description": "GPT-4 with 32K context window"
    },
    {
      "name": "gpt-35-turbo",
      "display_name": "GPT-3.5 Turbo (Azure)",
      "description": "GPT-3.5 Turbo hosted on Azure"
    }
  ],
  "headers": {
    "api-key": "{{api_key}}",
    "Content-Type": "application/json"
  },
  "icon": "☁️",
  "real_icon_url": "https://azure.microsoft.com/favicon.ico",
  "website": "https://azure.microsoft.com/en-us/products/ai-services/openai-service",
  "documentation": "https://learn.microsoft.com/en-us/azure/ai-services/openai/",
  "console_url": "https://portal.azure.com/#home",
  "registration_guide": "访问 [Azure Portal](https://portal.azure.com) 创建 Azure OpenAI 资源。需申请访问权限，审核需要数个工作日。",
  "notes": "企业级 AI 服务，支持私有网络和数据驻留。需要配置部署名称和实际资源 URL。详见 [Azure OpenAI 文档](https://learn.microsoft.com/azure/ai-services/openai/)。"
}