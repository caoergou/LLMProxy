{
  "provider": "azure",
  "name": "Azure OpenAI",
  "display_name": "Azure OpenAI",
  "description": "Microsoft Azure hosted OpenAI models with enterprise features",
  "base_url": "https://your-resource.openai.azure.com",
  "auth_type": "api-key",
  "request_format": "openai",
  "response_format": "openai",
  "cost_per_request": 0.002,
  "endpoints": [
    "/openai/deployments/{deployment-name}/chat/completions",
    "/openai/deployments/{deployment-name}/completions",
    "/openai/deployments/{deployment-name}/embeddings"
  ],
  "models": [
    {
      "name": "gpt-4",
      "display_name": "GPT-4 (Azure)",
      "model_family": "gpt-4",
      "description": "Azure托管的GPT-4模型，企业级安全",
      "capabilities": {
        "reasoning": true,
        "function_calling": true,
        "vision": false,
        "code_generation": true,
        "multimodal": false,
        "streaming": true,
        "context_length": 8192
      },
      "pricing": {
        "input_price": 0.03,
        "output_price": 0.06,
        "currency": "USD",
        "billing_unit": "1K_tokens"
      },
      "performance": {
        "avg_response_time": 2.8,
        "availability": 99.95
      },
      "limits": {
        "max_tokens": 4096,
        "rpm_limit": 3000,
        "tpm_limit": 40000
      }
    },
    {
      "name": "gpt-4-32k",
      "display_name": "GPT-4 32K (Azure)",
      "model_family": "gpt-4",
      "description": "Azure托管的GPT-4 32K上下文模型",
      "capabilities": {
        "reasoning": true,
        "function_calling": true,
        "vision": false,
        "code_generation": true,
        "multimodal": false,
        "streaming": true,
        "context_length": 32768
      },
      "pricing": {
        "input_price": 0.06,
        "output_price": 0.12,
        "currency": "USD",
        "billing_unit": "1K_tokens"
      },
      "performance": {
        "avg_response_time": 3.5,
        "availability": 99.9
      },
      "limits": {
        "max_tokens": 4096,
        "rpm_limit": 1500,
        "tpm_limit": 20000
      }
    },
    {
      "name": "gpt-35-turbo",
      "display_name": "GPT-3.5 Turbo (Azure)",
      "model_family": "gpt-3.5",
      "description": "Azure托管的GPT-3.5 Turbo模型",
      "capabilities": {
        "reasoning": true,
        "function_calling": true,
        "vision": false,
        "code_generation": true,
        "multimodal": false,
        "streaming": true,
        "context_length": 16385
      },
      "pricing": {
        "input_price": 0.001,
        "output_price": 0.002,
        "currency": "USD",
        "billing_unit": "1K_tokens"
      },
      "performance": {
        "avg_response_time": 1.5,
        "availability": 99.95
      },
      "limits": {
        "max_tokens": 4096,
        "rpm_limit": 8000,
        "tpm_limit": 160000
      }
    }
  ],
  "headers": {
    "api-key": "{{api_key}}",
    "Content-Type": "application/json"
  },
  "icon": "☁️",
  "real_icon_url": "https://azure.microsoft.com/favicon.ico",
  "website": "https://azure.microsoft.com/en-us/products/ai-services/openai-service",
  "documentation": "https://learn.microsoft.com/en-us/azure/ai-services/openai/",
  "console_url": "https://portal.azure.com/#home",
  "registration_guide": "访问 [Azure Portal](https://portal.azure.com) 创建 Azure OpenAI 资源。需申请访问权限，审核需要数个工作日。",
  "notes": "企业级 AI 服务，支持私有网络和数据驻留。需要配置部署名称和实际资源 URL。详见 [Azure OpenAI 文档](https://learn.microsoft.com/azure/ai-services/openai/)。"
}